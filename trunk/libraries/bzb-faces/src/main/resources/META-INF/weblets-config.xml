<?xml version="1.0" encoding="UTF-8"?>

<weblets-config xmlns="http://weblets.dev.java.net/config">  
  <weblet> 
    <weblet-name>bzb-faces</weblet-name>  
    <weblet-class>net.java.dev.weblets.packaged.PackagedWeblet</weblet-class>  
    <init-param> 
      <param-name>package</param-name>  
      <param-value>be.bzbit.framework.faces.resources</param-value> 
    </init-param> 
  </weblet>  
  <weblet-mapping> 
    <weblet-name>bzb-faces</weblet-name>  
    <url-pattern>/bzbFaces/*</url-pattern> 
  </weblet-mapping> 
</weblets-config>
